lgado/Documents/repo_vet_project_johan/pruebasImac_15_03_2022/src/Pages/Anava.jsx",[],[],["226","227"],"/Users/hectordelgado/Documents/repo_vet_project_johan/pruebasImac_15_03_2022/src/Pages/AnavaII.jsx",["228"],[],"import React from \"react\";\nimport ContentsTable from \"Components/ContentsTable\";\nimport ChapterLayout from \"Layouts/ChapterLayout\";\nimport SubChapter from \"Components/SubChapter\";\nimport Paragraph from \"Components/Paragraph\";\nimport SubTitle from \"Components/SubTitle\";\nimport ImagePlus from \"Components/ImagePlus\";\nimport Video from \"Components/Video\";\nimport ReferencesBlock from \"Components/ReferencesBlock\";\nimport ovejas from \"Media/photos_web/ovejas.JPG\";\nimport simplr from \"Media/anovas_excel/simple_reg.png\";\nimport simplr2 from \"Media/anovas_excel/simple_reg2.png\";\nimport simplr3 from \"Media/anovas_excel/simple_reg3.png\";\nimport simplr4 from \"Media/anovas_excel/simple_reg4.png\";\nimport multr from \"Media/anovas_excel/mult_reg.png\";\nimport multr2 from \"Media/anovas_excel/mult_reg2.png\";\nimport multr3 from \"Media/anovas_excel/mult_reg3.png\";\nimport regq from \"Media/anovas_excel/quad_reg.png\";\nimport regq2 from \"Media/anovas_excel/quad_reg3.png\";\nimport regq3 from \"Media/anovas_excel/quad_reg2.png\";\nimport regq4 from \"Media/anovas_excel/quad_reg4.png\";\nimport regq5 from \"Media/anovas_excel/quad_reg5.png\";\nimport inter from \"Media/anovas_excel/factor_reg.png\";\nimport inter2 from \"Media/anovas_excel/factor_reg2.png\";\nimport inter3 from \"Media/anovas_excel/factor_reg3.png\";\nimport dummy from \"Media/anovas_excel/dummy1.png\";\nimport dummy2 from \"Media/anovas_excel/dummy2.png\";\nimport dummy3 from \"Media/anovas_excel/dummy3.png\";\n\n\n\n\nconst AnavaII = () => {\n    return (\n      <ChapterLayout\n        chapter_title=\"7- El An√°lisis de Varianza (ANAVA)\"\n        chapter_image={ovejas}>\n\n<div id = \"tabla\" className = \"container bg-light\">\n    <ContentsTable\n            contenidos={[\n              {titulo: \"7.1-Que es el an√°lisis de varianza (ANAVA)\",\n                link: \"Anava#anava\",  },\n              {titulo: \"7.2- El ANAVA de un solo factor\",\n                link: \"Anava#unianova\",  },\n              { titulo: \"7.3- El ANAVA de dos factores sin replicaci√≥n\",\n                link: \"Anava#twoanova\", },\n              { titulo: \"7.4- El ANAVA de dos factores con replicaci√≥n\", link: \"Anava#twoanovarep\" },\n              { titulo: \"7.5- La regresi√≥n lineal\",\n                link: \"Anava#linearreg\", },\n              { titulo: \"7.6- La regresi√≥n lineal simple en Excel\", link: \"#reglinex\" },\n              {titulo: \"7.7- La regresi√≥n m√∫ltiple en Excel \",\n                link: \"#regmult\",\n              },\n              { titulo: \"7.8- La regresi√≥n cuadr√°tica en Excel\",\n                link: \"#regquad\",\n              },\n              { titulo: \"7.9- La regresi√≥n con interacci√≥n de elementos independientes en Excel\",\n              link: \"#reginter\",\n            },\n            { titulo: \"7.10- Variables nominales y construcci√≥n de variables ficticias\",\n            link: \"#dummyvars\",\n          },\n            ]}\n          />\n        </div>\n        <SubChapter id = \"reglinex\">\n            <SubTitle title = \"7.6- Regresi√≥n lineal simple en Excel\"/>\n            <Paragraph texto = {[\"En La tabla inferior podemos observar la producci√≥n de arroz en toneladas por hect√°rea/a√±o y el r√©gimen de lluvias en la India, referenciados en el paper de Sellam y Poovammal (2016).  Como vimos en el gr√°fico presentado en la \", <a href=\"Anava#linearreg\"><u>secci√≥n anterior</u></a>, \", parece existir una relaci√≥n lineal donde a mayor cantidad de lluvia, existe una disminuci√≥n de la cosecha (relaci√≥n inversa). Entonces, vamos a utilizar la herramienta \", <b>Data Analysis</b>, \" de Excel y generar un modelo sencillo donde el predictor ser√° el regimen de lluvias y la variable dependiente la producci√≥n de arroz.\" ]}/>\n\n        <ImagePlus\n          image={simplr}\n          texto=\"La tabla presenta el volumen de lluvias anual, la producci√≥n total de arroz en toneladas y el rendimiento por ha en India enttre los a√±os 2000 a 2010.\"\n          width=\"35%\"\n          max_zoom = \"60%\"\n        />\n        <Paragraph texto = {[\n            <center> <h4>La tabla presenta el volumen de lluvias anual, la producci√≥n total de arroz en toneladas y el rendimiento por ha en India enttre los a√±os 2000 a 2010.</h4> </center>, <br/>, <br />,\n            \"Una vez que los datos est√°n ubicados en una tabla simple como la que se muestra arriba, del men√∫ datos, seleccionamos \", <b>Data Analysis</b> ,\" y la opci√≥n regresi√≥n y de ah√≠ se desprende la siguiente hoja de di√°logo:\"\n        ]}/>\n        <ImagePlus\n          image={simplr2}\n          texto=\"Men√∫ de di√°logo para seleccionar opciones\"\n          width=\"35%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\"La el men√∫ que se aprecia el recuadro superior nos pide seleccionar el rango donde se encuentra la variable Y (dependiente), luego la variable X (predictiva), el nivel de Confianza (para este caso  por defecto vamos a dejar el 95% o sea una significancia del 5%) y finalmente seleccionamos la ubicaci√≥n para la generaci√≥n del reporte. Hacemos click en OK para obtener un reporte con tres tablas como el que se observa a continuaci√≥n.\"]}/>\n\n        <ImagePlus\n          image={simplr3}\n          texto=\"Reporte generado por Excel\"\n          width=\"60%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\n            \"La primera tabla del reporte de Excel (cuadro superior) nos muestra varios resultados. El valor que mas nos interesa aqu√≠ es el R\",<sup>2</sup>, \"como hemos visto anteriormente este valor  mide hasta que nivel las variables utilizadas para predecir explican la variaci√≥n en los resultados de la variable dependiente. En este caso el R\",<sup>2</sup>,\" es de 0.61, entonces esto significa que el 61 de la variaci√≥n se explica por el r√©gimen de lluvias. Entre mas alto sea el valor de R\",<sup>2</sup>,\"  mejor ser√° el ajuste del modelo.  Una forma sencilla de comprobar un R\",<sup>2</sup> ,\"es que en el gr√°fico de dispersi√≥n los puntos est√°n cercanos a la l√≠nea diagonal, o en otras palabras casi que los puntos arman la l√≠nea.  Es l√≥gico que la lluvia no explique toda la variabilidad en los resultados, sin embargo, como vemos lleva una parte alta en la responsabilidad. Parece que un exceso en el r√©gimen de lluvias va a causar una disminuci√≥n en la cosecha. \",<br/>,\n            \"La segunda tabla corresponde al an√°lisis de varianza. En esta parte es donde vamos a comprobar nuestra hip√≥tesis nula  (H\",<sub>0</sub>,\"), que para la regresi√≥n simple seria que la diagonal de la l√≠nea de mejor ajuste es igual a cero. Es decir, no hay ning√∫n efecto de la variable independiente sobre la variable dependiente. De esta tabla debemos sobre todo fijarnos en el valor de F y su significancia.  En este caso la significancia de F (0.0046) es inferior al nivel de significancia seleccionado (0.05) por tanto tenemos que rechazar la H\",<sub>0</sub>,\"y aceptar la H\",<sub>1</sub>, \", es decir el r√©gimen de lluvias tiene una afectaci√≥n sobre la producci√≥n anual de arroz/ha.\",<br/>,\n            \"La tercera tabla del an√°lisis de varianza nos presenta los coeficientes y errores est√°ndar tanto del intercepto y del valor de X o sea la lluvia anual.  Estos coeficientes son los que podremos utilizar para estimar los valores de Y reemplazandolos en la ecuaci√≥n de \",<a href=\"anava.html#linearreg\"><u>la regresi√≥n simple</u></a>,\". Es importante que cuando se haga predicci√≥n la realicemos dentro del rango de los valores de X hallados en los datos originales (interpolaci√≥n).  Predecir valores de Y fuera de este rango de valores observados (extrapolaci√≥n) puede dar resultados inesperados pues no sabemos como se comportan los datos por fuera de del rango establecido. \"\n        ]}/>\n\n        <ImagePlus\n          image={simplr4}\n          texto=\"Gr√°fico de dispersi√≥n\"\n          width=\"50%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\n            <center> <h4> Gr√°fico de dispersi√≥n que muestra los valores de Y (azul) frente a los valores estimados y por el modelo de regresi√≥n lineal simple </h4> </center>, <br />, \"Al seleccionar la opci√≥n de Regresi√≥n en la herramienta de an√°lisis de datos, La hoja de di√°logo nos permite seleccionar la opci√≥n de graficar los resultados del modelo y comparar con los resultados de la tabla como se observa en el gr√°fico superior.  Esto nos permite ver el ajuste y visualizar que tan preciso puede ser el modelo al utilizarlo en la pr√°ctica.\"\n        ]}/>\n\n        <center><SubTitle title = \"Regresi√≥n lineal simple en Excel\"/></center>\n      \n      <Video id=\"K5cBjru20RU\" title=\"Regresi√≥n lineal simple en Excel\" />\n      </SubChapter>\n\n    <SubChapter id = \"regmult\">\n        <SubTitle title = \"7.7- Regresi√≥n m√∫ltliple en Excel\"/>\n        <Paragraph texto = {[\n            \n            \"Cuando estamos buscando medir el impacto de varias variables idependientes sobre una dependiente, una alternativa ser√≠a crear un modelo de regresi√≥n lineal simple para cada predictor, pero esto no ser√≠a acertado ya que no est√° claro como hacer una sola predicci√≥n para dos o mas variables y cada uno de los modelos simples estar√≠a ignorando el efecto de las otras variables (Macdondald 2009). Como alternativa se utiliza un modelo similar al de la regresi√≥n lineal simple pero cada uno de los predictores tiene un coeficiente de inclinaci√≥n diferente.\", <br />,\n            \"La regresi√≥n m√∫ltiple no es muy diferente de la regresi√≥n simple.  Esta se utiliza cuando existe mas de un predictor o variable independiente (variable X). Generalmente este modelo es mucho mas com√∫n que la regresi√≥n simple. La diferencia est√° en que esta tiene mas elementos independientes (al menos dos) y a la vez es  mas compleja de visualizar ya que no podr√≠amos manejar el gr√°fico sobre un plano cartesiano sino al menos en un cubo tridimensional. Por tanto, visualizar el impacto de una variable sobre el resultado no es tan obvio ya que medimos el impacto de multiples variables independientes sobre una variable dependiente. Sin embargo, las herramientas de an√°lisis nos permiten entender los resultados.\", \n        ]}/>\n\n        <ImagePlus\n          image={multr}\n          texto=\"M√≥delo de regresi√≥n m√∫ltiple\"\n          width=\"40%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\n            \"En la ecuaci√≥n superior el valor de Y esta dado por ùõÉ\", <sub>0</sub>,\" que es el intercepto o el valor de Y cuando X vale 0 y los valores de ùõÉ\",<sub>n</sub>,\" representan cada una de las constantes para las pendiente de los multiples valores de X\",<sub>n</sub> ,\"cuando los dem√°s predictores se mantienen fijos y ùõÜ es el error. T√≠picamente se asume que el error es independiente de X.\" ,<br/>,\n            \"Entonces resumiendo el prop√≥sito de una regresi√≥n m√∫ltiple es encontrar una ecuaci√≥n que prediga el valor mas ajustado de Y como una funci√≥n lineal de m√∫ltiples variables X. La regresi√≥n m√∫ltiple puede ser utilizada bien para predicci√≥n o para entender las relaciones funcionales entre las variables dependientes e independientes, o sea para entender como se afecta la variable dependiente cuando se realizan cambios en los predictores. Como saber si el modelo tiene un buen ajuste de los datos? Por el R\",<sup>2</sup>,\" , el coeficiente de m√∫ltiple determinaci√≥n. \", <br />,\n            \"Para ilustrar la regresi√≥n m√∫ltiple tomemos un ejemplo del curso de m√©todos estad√≠sticos II de la Universidad de McGill (Roger Cue,20?). Para ello tenemos la informaci√≥n de un grupo de 50 vacas que fue alimentado con diferentes dietas utilizando diferentes niveles de alimentaci√≥n (50 hasta 140 lb de henolaje) y con varias densidades de energ√≠a (0.8 hasta 1.6). El rendimiento de leche para la lactaci√≥n medido en kgs se muestra en la siguiente tabla\"\n        ]}/>\n        <ImagePlus\n          image={multr2}\n          texto=\"Fragmento de la tabla que presenta Densidad de Energia, Cantidad de Alimento y Producci√≥n de leche en 50 vacas de orde√±o (fuente: Cue, 2019)\"\n          width=\"40%\"\n          max_zoom = \"60%\"\n        />\n        <Paragraph texto = {[\n            <center> <h4> Fragmento de la tabla que presenta Densidad de Energia, Cantidad de Alimento y Producci√≥n de leche en 50 vacas de orde√±o (fuente: Cue, 2019)</h4></center>, <br />,\n            \"Una vez tenemos los datos en una tabla como se presenta en la parte superior, nos ubicamos en la herramienta \",  <b>Data Analysis</b> ,  \",  de Excel y seleccionamos la opci√≥n \", <b>regression</b> , \" o regresi√≥n y en la hoja de di√°logo seguimos los mismos pasos que para la regresi√≥n simple, solo que para las variables independientes X, el rango no es de una sola columna sino dos o mas dependiendo del m√≥delo, en este caso nuestras variables independientes son la cantidad de alimento y la densidad de la energ√≠a.  Una vez que damos OK obtendremos el siguiente reporte:\",\n        ]}/>\n        <ImagePlus\n          image={multr3}\n          texto=\" Informe de regresi√≥n m√∫ltple generado por Excel Data Analysis\"\n          width=\"50%\"\n          max_zoom = \"60%\"\n        />\n        <Paragraph texto = {[ \"El informe que presenta Excel esta dividido en tres tablas, En la primera nos presenta el valor de R\",<sup>2</sup>,\" que para este caso es del 84%. Tambi√©n podemos conocer el valor del R Multiple este valor mide la cantidad de variaci√≥n en la respuesta variable que puede ser explicada por las variables usadas para predecir el valor de Y.  Cuando se incrementan las variables como resultado R\", <sup>2</sup> ,\" siempre va a aumentar, teniendo presente que un predictor siempre va a explicar alguna proporci√≥n de la varianza. El R\", <sup>2</sup>,\" ajustado se usa como un control contra el incremento de variables predictivas (independientes) y genera una penalidad para el numero de predictores en el modelo. Si existe mucha diferencia entre el R\",  <sup>2</sup>, \" ajustado y el R\" , <sup>2</sup>, \" significa que el modelo esta sobre ajustado (hay demasiadas variables).\", <br />,\n        \"La segunda tabla nos presenta el an√°lisis de varianza como tal y como podemos ver en este caso la significancia de F es inferior 0.0001 por tanto el modelo es estadisticamente significativo.  Finalmente, la tercera tabla detalla los componentes del modelo. Los valores de P para ambos coponentes son inferiores a 0.0001 por lo tanto son estadisticamente significativos y podemos incluirlos en el modelo. \",\n        <br />,\n        <h4> <center>Regresi√≥n con multiples variables independientes </center></h4>\n        ]}/>\n\n      \n      <Video id=\"PtucNTB4-1w\" title=\"Regresi√≥n con multiples variables independientes \" />\n\n    \n    </SubChapter>\n\n    <SubChapter id = \"regquad\">\n        <SubTitle title = \"7.8- Regresi√≥n cuadr√°tica en Excel\"/>\n        <Paragraph texto = {[\n          \"T√≠picamente la funci√≥n que describe la regresi√≥n de X y Y es una regresi√≥n lineal, sin embargo, algunas veces el efecto no es lineal y es importante aplicar otros tipos de regresi√≥n. Una regresi√≥n com√∫n es la regresi√≥n cuadr√°tica, esta se caracteriza porque la relaci√≥n toma la forma de una curva semejante a una ‚à™ o ‚à© (Howard). \", <br/>,\n          \"En biolog√≠a muchas relaciones no son completamente lineales y a menudo puede existir una relaci√≥n curvil√≠nea o cuadr√°tica, con un intermedio que puede ser el √≥ptimo dependiendo de si es un m√°ximo o un m√≠nimo lo que se desea. Por ejemplo ocurre en ganader√≠a lechera ya que la mortalidad de los terneros y su relaci√≥n con el peso al nacer tiene una relaci√≥n curvil√≠nea. Terneros con peso muy bajo tienen mayor riesgo de muerte mientras terneros con un peso promedio tienen baja probabilidad de muerte y terneros muy pesados de nuevo tienen mayor probabilidad  de muerte. Entonces parece que existir√≠a una relaci√≥n cuadr√°tica (Cue 2019).\" , <br/>,\n          \"La ecuaci√≥n de la regresi√≥n con un elemento cuadr√°tico se expresa asi:\", \n\n        ]}/>\n\n        <ImagePlus\n          image={regq}\n          texto=\"Ecuaci√≥n cuadr√°tica\"\n          width=\"30%\"\n          max_zoom = \"50%\"\n        />\n        <Paragraph texto = {[\n          \"Donde Y es la variable dependiente, ¬µ es el intercepto, ùõÉ\", <sub>1</sub>,\" es el coeficiente de inclinaci√≥n de la diagonal, X\", <sub>i</sub>,\n          \" es la variable independiente, ùõÉ\", <sub>2</sub>, \"es el coeficiente de la variable X\", <sub>i</sub>, \"elevada al cuadrado, X\", <sup>2</sup>, \"es el cuadrado de la variable independiente y ùúÄ representa el error.\", <br/>, \"Para este ejemplo vamos a seguir con los datos tomados de Cue (2019) donde un grupo de 50 vacas fue alimentado con dietas utilizando varios niveles de alimentaci√≥n (50 hasta 140 lb de henolaje) con varias densidades de energ√≠a (0.8 hasta 1.6).\", <br />,\n          \"Cuando hacemos un gr√°fico de dispersi√≥n de los resultados de leche (Y) obtenidos por el valor cuadr√°tico de la dieta podemos observar que a pesar de que hay una tendencia creciente en la producci√≥n, en cierto punto parece que hay una meseta y luego una cierta inflexi√≥n. Es por ello vamos a ensayar este valor cuadr√°tico en el modelo.\"\n        ]}/>\n     \n\n        <Paragraph texto = {[\n          <center> <h4>Gr√°fico de dispersi√≥n que muestra la ingesta de alimento(X) por la producci√≥n de leche (Y)</h4></center>,\n          <br/>,\n          \"La tabla inferior presenta un fragmento de la informaci√≥n donde se incluye el valor de la densidad de la energ√≠a y la ingesta de alimento por vaca seg√∫n el tratamiento asignado y la producci√≥n de leche.  Se incluyo tambi√©n el valor cuadr√°tico de la dieta, para ello en Excel se inserta una columna y simplemente se eleva el valor al cuadrado como se observa en la barra de f√≥rmulas de Excel en la tabla.\"\n\n        ]}/>\n\n        <ImagePlus\n          image={regq3}\n          texto=\"Tabla de datos de producci√≥n lechera incorporando la variable cuadr√°tica en Excel \"\n          width=\"40%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\n          \"Entonces para realizar la regresi√≥n en Excel, una vez que hemos incluido el elemento cuadr√°tico, vamos a la secci√≥n datos y seleccionamos la herramienta \", <b>Data Analysis</b> ,\"tal como lo hemos hecho anteriormente.  Seleccionamos la opci√≥n Regresi√≥n y en la hoja de dialogo seleccionamos el Y (producci√≥n de leche), y las variables predictivas o X, este caso Densidad de Energia, Volumen de Dieta y el Factor Cuadr√°tico. Ojo, siempre que incluyamos un valor cuadr√°tico, es necesario incluir el valor original. \"\n        ]}/>\n        <ImagePlus\n          image={regq4}\n          texto=\"Cuadro de di√°logo \"\n          width=\"50%\"\n          max_zoom = \"60%\"\n        />\n\n        <Paragraph texto = {[\n          \"Damos OK y Excel nos genera el siguiente reporte:\"\n        ]}/>\n      <ImagePlus\n          image={regq5}\n          texto=\"Reporte generado por Excel\"\n          width=\"60%\"\n          max_zoom = \"60%\"\n        />\n\n      <Paragraph texto = {[\n         <center><h4>Reporte generado por Excel, listo para ser interpretado</h4></center> ,\"La tabla superior nos presenta el R\",<sup>2</sup>,\n         \"del modelo es del 0.87 o en otras palabras las variables independientes determinan el valor de la variable independiente en un 87% lo cual es muy bueno y esto se confirma con el R\",<sup>2</sup>,\" ajustado ya que estan muy cercanos el uno del otro. Vemos que este modelo ajusta mejor que el modelo de regresi√≥n m√∫ltiple sin el componente cuadr√°tico ya que el valor R\",<sup>2</sup>,\"para este modelo fuel del 84%. En la segunda tabla vemos que el modelo es significativo (p < 0.0001) y en la tercera tabla cada uno de los elementos del modelo son tambien significativos (P-value < 0.0001) incluido el efecto cuadr√°tico de la dieta, por tanto, no podemos descartarlo del modelo.  Cuando el valor cuadr√°tico es significativo este es el que se considera significativo, sin embargo el valor simple no se debe eliminar del modelo, simplemente se asume como significativo el valor cuadr√°tico. Tambi√©n en la tercera tabla podemos ver los coeficientes de variaci√≥n para cada uno de los elementos del modelo con sus errores est√°ndar.\"\n        ]}/>\n\n      <center><SubTitle title = \"Modelo de Regresi√≥n cuadr√°tica\"/></center>\n      \n      <Video id=\"QYZKsT9xTrg\" title=\"Regresi√≥n cuadr√°tica en Excel\" />\n\n    </SubChapter>\n\n    <SubChapter id = \"reginter\">\n      <SubTitle title = \"7.9- Regresi√≥n con interacci√≥n de dos elementos independientes en Excel\"/>\n        <Paragraph texto = {[\n          \"Algunas veces la variable dependiente depende no solo de las variables independientes sino de la interacci√≥n entre las variables el modelo. Por ejemplo, si estamos evaluando la producci√≥n de leche y en nuestro modelo estamos incluyendo el efecto de la dieta y la ingesta del alimento (Cue, 2019), es posible que exista una interacci√≥n entre la dieta y el lote, por ello debemos incluir esta interacci√≥n en nuestro modelo. La ecuaci√≥n entonces se representar√≠a de la siguiente manera:\"\n        ]}/>\n\n      <ImagePlus\n          image={inter}\n          texto=\"M√≥delo con interacci√≥n\"\n          width=\"30%\"\n          max_zoom = \"50%\"\n        />\n\n        <Paragraph texto = {[\n          \"Donde Y es la variable dependiente, ùùÅ es el intercepto, ùú∑\", <sub>1</sub>,\" es el coeficiente de inclinaci√≥n de la variable independiente X\",\n          <sub>1</sub>,\" (igual para Œ≤\",<sub>2</sub>, \" X\",<sub>2</sub>,\"), ùú∑\",<sub>3</sub>,\" es el coeficiente de la inclinaci√≥n de la interacci√≥n entre X\", <sub>1</sub>, \" y X\", <sub>2</sub>, \" y ùùê representa el error.\" , <br/>, \"Supongamos que queremos continuar con el ejemplo anterior y deseamos saber si existe interacci√≥n entre la cantidad de alimento ingerido y la densidad de la energ√≠a afectando la producci√≥n de leche. Entonces para capturar la interacci√≥n entre ingesta alimentaria y densidad de la energ√≠a, insertamos una variable independiente que llamaremos ‚Äúinteracci√≥n‚Äù como se observa en la tabla. La interacci√≥n simplemente es el producto de ingesta alimentaria y densidad de la energ√≠a para cada una de las observaciones. En la barra de operaciones se observa la f√≥rmula de la multiplicaci√≥n.\"\n        ]}/>\n\n      <ImagePlus\n          image={inter2}\n          texto=\"Tabla de datos de producci√≥n lechera incorporando la interacci√≥n en Excel\"\n          width=\"30%\"\n          max_zoom = \"50%\"\n        />\n\n        <Paragraph texto = {[\n          <center><h4> Tabla de datos de producci√≥n lechera incorporando la interacci√≥n en Excel</h4></center>,\n          <br />,\n          \"Una vez hemos hecho la interacci√≥n para cada observaci√≥n, vamos a la herramienta ‚ÄúData Analysis‚Äù de Excel y seleccionamos la opci√≥n Regresi√≥n tal como lo hemos visto anteriormente. Una vez que damos OK, podemos ver el reporte de Excel que se presenta a continuaci√≥n:\"\n        ]}/>\n      <ImagePlus\n          image={inter3}\n          texto=\"Reporte generado por Excel\"\n          width=\"60%\"\n          max_zoom = \"80%\"\n        />\n\n      <Paragraph texto = {[\n          <center><h4> Reporte generado por Excel </h4></center>,\n          <br />,\n          \"Como vemos el reporte generado por Excel es similar al de todas las anteriorers regresiones, por ello no vamos a entrar en detalle, solo vemos que el R\", <sup>2</sup>,\" es inferior al modelo con el componente cuadr√°tico, por tanto sabemos que este m√≥delo seguramente no ser√° el modelo a escoger.  Al observar el an√°lisis de varianza vemos que la interacci√≥n entre la Energ√≠a y la Dieta no son significativas (P-value = 0.43) por tanto debemos descartar la interacci√≥n, mientras que las dem√°s variables si son significativas. En caso de que la interacci√≥n fuera significativa (P-value < 0.05) al describir el modelo, el efecto significativo a describir es la interacci√≥n y no los factores de esta as√≠ estos fueran significativos.\"\n        ]}/>  \n\n    \n    <center><SubTitle title = \"Regresi√≥n con interacci√≥n de dos variables independientes (factorial)\"/></center>\n      \n      <Video id=\"Ma9TjV1kuFM\" title=\"Regresi√≥n cuadr√°tica en Excel\" />\n      </SubChapter>\n\n      <SubChapter id = \"dummyvars\">\n      <SubTitle title = \"7.10- Variables nominales y construcci√≥n de variables ficticias\"/>\n      <Paragraph texto = {[\n      \"Como hemos visto en los casos anteriores, en las regresiones lineales utilizamos variables continuas.  De acuerdo con el efecto que tienen estos predictores pueden haber interacciones o efectos cuadraticos (tambien pueden haber efectos c√∫bicos pero no seran tratados aqu√≠. Tambi√©n es posible utilizar variable s\", <a href=\"Post#raw\"> <u>nominales</u> </a>, \" en el an√°lisis de regresi√≥n m√∫ltiple. Por ejemplo, variables nominales como el n√∫mero de la lactancia en ganado vacuno o el n√∫mero del parto en cerdas de cr√≠a. Tambi√©n dic√≥tomas como el sexo en ganado de ceba que pueden jugar un importante papel en consumo de alimento o la ganancia de peso, por tanto no pueden ser excluidas.  Estas variables siempre se deben tratar como como un c√≥digo binario 0 y 1 y para ello se crean las variables ficticias (dummy variables en ingl√©s).\", <br />,\n      \"Como el modelo solo acepta valores de 0 y 1, cuando tenemos variables con mas de una categor√≠a debemos crear variables ficticas que representen las comparaciones entre los diferentes grupos (razas, regi√≥n etc.). El grupo de variables ficticias son consideradas en el modelo de regresi√≥n simult√°neamente como un conjunto de variables independientes.  Suponga que queremos medir en una granja de cerdos los niveles en hormonas de la reproducci√≥n en las diferentes hembras que hay en la granja. Actualmente en la granja hay cerdas de las razas Pietrain, Landrace, Duroc y Poland-China. Esta variable tendr√≠a 4 categ√≥rias pero se construyen solo tres variables ficticias.  Para considerar entonces raza como predictor en un modelo de regresi√≥n, se deben crear tres variables indicadoras (una menos que el total) para representar los cuatro diferentes grupos. Para crear el conjunto de variables, primero entonces hay que decidir un grupo referencia o categor√≠a, es decir en este caso una raza que se comparara contra los otros grupos. Es por esto que se construye una una menos ya que si todas las demas variables independientes son 0, se entiende entonces que los individuos pertenecen a la categoria base. Las variables ficticias son creadas para los dem√°s grupos y se codifica con 1 para los participantes que pertenecen a ese grupo y todos los dem√°s son c√≥digo 0.  En el modelo de regresi√≥n m√∫ltiple, los coeficientes de regresi√≥n asociados con cada una de las variables ‚Äúdummy‚Äù son interpretadas como la diferencia esperada entre la media esperada de ese grupo comparada con el grupo de referencia, cuando todos los otros predictores se mantienen constantes. \", <br/>,\n      \"Para variables dic√≥tomas como sexo bastar√≠a crear una sola variable por ejemplo la variable ‚ÄúHembra‚Äù entonces asignamos el valor 1 en caso de que sea hembra y 0 para macho.  No es necesario crear la variable ‚ÄúMacho‚Äù pues ya sabemos que si es 0 en la variable ‚ÄúHembra‚Äù es macho y entonces en el modelo estar√≠amos utilizando como grupo referencia la variable macho y comparando en cuanto difiere la hembra. \", <br />,\n      \"Ejemplo en Excel: Tenemos 1657 registros de vacas lecheras de un solo hato, y deseamos establecer cual es la diferencia entre lactancias de la 1¬™ a la 3¬™, tambi√©n tenemos el promedio logar√≠tmico del recuento de c√©lulas som√°ticas para cada lactaci√≥n.  Entonces queremos establecer un modelo donde se incluya la lactancia como variable ficticia donde el grupo base seria la 1¬™ lactaci√≥n y crear√≠amos las variables dummy para la 2¬™ y 3¬™ lactancias. Tambi√©n incluiremos el efecto de las c√©lulas som√°ticas para ver si tienen alguno valor predictivo sobre la producci√≥n de leche.\"\n\n      ]}/>\n      <ImagePlus\n          image={dummy}\n          texto=\"Fragmento de la tabla  de datos de producci√≥n lechera, lactancias y promedio \n          logar√≠tmico de recuento de c√©lulas som√°ticas.\"\n          width=\"40%\"\n          max_zoom = \"80%\"\n        />\n\n      <Paragraph texto = {[\n        <center> <h4> Fragmento de la tabla  de datos de producci√≥n lechera, lactancias y promedio logar√≠tmico de recuento de c√©lulas som√°ticas.</h4> </center>, <br />,\n        \"El primer paso es insertar dos columnas para incluir las variables ficticias. Para crearlas en Excel b√°sicamente podemos utilizar la funci√≥n =IF, para la lactancia 2 entonces nos ubicamos en la celda insertada para este prop√≥sito y escribimos la funci√≥n tal como se presenta en la siguiente tabla, donde si B2 (celda donde esta la lactancia) = 2, entonces asigne valor 1, de lo contrario asigne valor 0.  Copiamos la f√≥rmula en la columna y repetimos para lactancia 3.  \"\n      ]}/>\n\n        <ImagePlus\n          image={dummy2}\n          texto=\"Construcci√≥n de las variables ficticias\"\n          width=\"40%\"\n          max_zoom = \"80%\"\n        />\n      <Paragraph texto = {[\n       <center> <h4>construcci√≥n de las variables ficticias para la 2<sup>a</sup> y 3<sup>a</sup> lactaci√≥n.</h4></center>, <br />,\n        \"Una vez que tenemos las variables construidas, seleccionamos la herramienta ‚ÄúData Analysis‚Äù del men√∫ datos de Excel y de ah√≠ seleccionamos Regresi√≥n.  Seleccionamos el rango de la variable dependiente Y que en este caso es la producci√≥n de leche y seleccionamos las variables independientes X que son las variables ficticias que acabamos de constuir y el conteo de c√©lulas som√°ticas, sin olvidar que los datos tienen etiquetas y finalmente asignamos donde queremos que se genere el reporte.  \"\n      ]}/>\n\n      <ImagePlus\n          image={dummy3}\n          texto=\"Reporte generado por Excel \"\n          width=\"60%\"\n          max_zoom = \"80%\"\n        />\n       <Paragraph texto = {[\n       <center> <h4>Reporte generado por Excel </h4></center>, <br />,\n        \"El reporte generado por Excel nos presenta las tres tablas que ya conocemos.  En la tabla superior podemos ver el valor de R\",<sup>2</sup>,\n        \"que en este caso es de 0.368 es decir, las variables independientes describen en un 37% el resultado de la variable independiente.  No es muy alto pero solo quer√≠amos conocer el peso de las lactancias as√≠ como el impacto de las de c√©lulas som√°ticas.   La tabla 2 nos muestra el ANAVA aqu√≠ podemos ver que el modelo es significativo, si vemos la √∫ltima columna de la derecha, el valor  de F es de 2.47 exponente -164, por tanto es muy pero muy inferior al 0.05% establecido como significancia (recordemos que si la confianza seleccionada es del 95% entonces la significancia ser√° del 5% o 0.05). Finalmente, en la tercera tabla podemos observar los coeficientes. Entonces para el intercepto que seria un animal de 1\", <sup>a</sup>, \" lactancia la producci√≥n de leche estar√≠a en 8310 kg con un error est√°ndar de 82.  Si el animal es de lactancia 2 entonces se adicionar√≠a 1755 kg de leche con un error est√°ndar de 78 kg y si el animal es de lactancia 3 entonces al intercepto se le adicionan 2472 kg con un error est√°ndar de 91. Las dos variables dummy (lact 2 y lact 3) son significativas con P - values muy inferiores a 0.05.  En cuanto al valor logar√≠tmico del recuento de c√©lulas som√°ticas vemos que tambi√©n tiene un efecto significativo, pero este es negativo sobre la producci√≥n de leche. Como vemos cada punto en que aumentan las c√©lulas som√°ticas, disminuye en 224 kg la producci√≥n de leche. \"\n      ]}/>\n    <center><SubTitle title = \"Construcci√≥n de variables tipo dummy en Excel\"/></center>\n      \n      <Video id=\"Wr4vK5yB4Bw\" title=\"Construcci√≥n de variables tipo dummy en Excel\" />\n\n      </SubChapter>\n\n      <ReferencesBlock references = {[\n        \n    <a href=\"https://sphweb.bumc.bu.edu/otlt/MPH-Modules/BS/BS704_Multivariable/BS704_Multivariable7.html\"><u>* Boston University School of Public Health. 2013, Multivariable Regression. </u></a>,\n  \"* Chao, L.L., 1978. Estad√≠stica para las ciencias administrativas. McGraw-Hill Interamericana.\",\n   <a href=\"http://animsci.agrenv.mcgill.ca/StatisticalMethodsII/\"><u>* Cue R., 2019  Course of Statistical MethodsII, McGill University.</u></a> ,\n   <a href=\"https://www.statology.org/linear-regression-real-life-examples/\"><u>* Zach, 2020.  Examples of using linear regression in linear life. En portal Statology.com</u> </a>,\n \" * Guerrero, H., 2010. Excel data analysis: modeling and simulation. Springer Science & Business Media.\",\n  <a href=\"https://mattchoward.com/quadratic-regression-in-excel/\"><u>* Howard M., Consultado Junio 2021. Quadratic Regression in Excel. En portal Mattchoward.com/</u></a> ,\n  \"* McDonald, J.H., 2009. Handbook of biological statistics (Vol. 2, pp. 6-59). Baltimore, MD: sparky house publishing.\",\n \" * Pfaffenberger, R.C. and Patterson, J.H., 1977. Statistical methods for business and economics. Homewood, IL: Richard D. Irwin.\",\n  \"* Sellam, V. and Poovammal, E., 2016. Prediction of crop yield using regression analysis. Indian Journal of Science and Technology, 9(38), pp.1-5.\",\n  <a href=\"https://www.real-statistics.com/multiple-regression/interaction/\"><u>* Zaiontz C., Consultado Junio 2021. Interaction. En portal Real Statistics using Excel. </u></a>\n  \n      ]}/>\n\n        </ChapterLayout>\n\n    );\n        };\n\n        export default AnavaII","/Users/hectordelgado/Documents/repo_vet_project_johan/pruebasImac_15_03_2022/src/Pages/Referencias.jsx",[],[],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"229","replacedBy":"233"},{"ruleId":"231","replacedBy":"234"},{"ruleId":"235","severity":1,"message":"236","line":2,"column":8,"nodeType":"237","messageId":"238","endLine":2,"endColumn":19},{"ruleId":"229","replacedBy":"239"},{"ruleId":"231","replacedBy":"240"},{"ruleId":"229","replacedBy":"241"},{"ruleId":"231","replacedBy":"242"},{"ruleId":"229","replacedBy":"243"},{"ruleId":"231","replacedBy":"244"},{"ruleId":"235","severity":1,"message":"245","line":6,"column":8,"nodeType":"237","messageId":"238","endLine":6,"endColumn":21},{"ruleId":"246","severity":1,"message":"247","line":12,"column":13,"nodeType":"248","endLine":12,"endColumn":37},{"ruleId":"235","severity":1,"message":"249","line":2,"column":10,"nodeType":"237","messageId":"238","endLine":2,"endColumn":20},{"ruleId":"235","severity":1,"message":"250","line":3,"column":10,"nodeType":"237","messageId":"238","endLine":3,"endColumn":23},{"ruleId":"251","severity":1,"message":"252","line":34,"column":15,"nodeType":"253","messageId":"254","endLine":34,"endColumn":17},{"ruleId":"246","severity":1,"message":"247","line":28,"column":5,"nodeType":"248","endLine":28,"endColumn":56},{"ruleId":"246","severity":1,"message":"247","line":29,"column":5,"nodeType":"248","endLine":31,"endColumn":39},{"ruleId":"235","severity":1,"message":"255","line":2,"column":10,"nodeType":"237","messageId":"238","endLine":2,"endColumn":14},{"ruleId":"246","severity":1,"message":"247","line":6,"column":9,"nodeType":"248","endLine":6,"endCol